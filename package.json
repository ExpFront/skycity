{
  "name": "angular-webpack-workflow",
  "version": "4.0.0",
  "description": "A workflow for Angular made with Webpack",
  "keywords": ["angular", "webpack", "babel", "postcss", "jade", "livereload", "karma", "protractor", "jasmine",
    "workflow", "e2e"],
  "scripts": {
    "build": "webpack --config webpack.build.js --bail -p",
    "dev": "sudo webpack-dev-server --content-base './src/content/' --history-api-fallback --inline --progress",
    "test": "sudo karma start",
    "test:live": "sudo karma start --auto-watch --no-single-run",
    "stats": "sudo webpack --json --profile > stats.json",
    "clean": "rimraf ./public",
    "e2e": "node ./node_modules/.bin/protractor",
    "e2e:live": "protractor --elementExplorer",
    "ci": "npm run e2e && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zombiQWERTY/angular-webpack-babel-postcss-jade-livereload-workflow.git"
  },
  "author": "Jesus Rodriguez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zombiQWERTY/angular-webpack-babel-postcss-jade-livereload-workflow/issues"
  },
  "homepage": "https://github.com/zombiQWERTY/angular-webpack-babel-postcss-jade-livereload-workflow",
  "dependencies": {
    "angular": "^1.5.0",
    "angular-route": "latest",
    "fastclick": "latest"
  },
  "devDependencies": {
    "angular-mocks": "^1.4.8",
    "autoprefixer": "^6.0.3",
    "babel-core": "^6.2.1",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.1.18",
    "css-loader": "^0.23.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "html-loader": "^0.4.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "install": "^0.4.2",
    "isparta-instrumenter-loader": "^1.0.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-spec-reporter": "0.0.23",
    "karma-webpack": "^1.7.0",
    "node-libs-browser": "^0.5.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.8",
    "postcss-hexrgba": "^0.2.0",
    "postcss-import": "^7.1.3",
    "postcss-inline-comment": "^2.0.0",
    "postcss-loader": "^0.8.0",
    "precss": "^1.4.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "selenium-standalone": "^4.9.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  }
}
